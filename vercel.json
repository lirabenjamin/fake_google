{
  "functions": {
    "api/track-click.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri",
    "MONGODB_DATABASE": "@mongodb-database",
    "MONGODB_COLLECTION": "@mongodb-collection"
  }
}